/* 介绍页面 */

.welcome {
  box-sizing: border-box;
  width: 560rpx;
  height: 800rpx;
  position: fixed;
  top: 330rpx;
  left: 95rpx;
  z-index: 68;
  background: #fff;
}

.top-bar {
  box-sizing: border-box;
  width: 100%;
  height: 50rpx;
  background: #ea889e;
  display: flex;
  justify-content: flex-end;
  color: #fff;
  padding-right: 16rpx;
  padding-top: 8rpx;
}

.closeIcon {
  width: 38rpx;
  height: 38rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  margin-top: 20rpx;
}
.title2{
  font-size: 26rpx;
}
scroll-view.content {
  box-sizing: border-box;
  font-size: 24rpx;
  font-weight: bold;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 20rpx;
  width: 560rpx;
  height: 620rpx;
}

.bg-page {
  background: #fff;
  opacity: 0.7;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 67;
}

.bg-img {
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto;
  margin-top: -400rpx;
}

