/**index.wxss**/

.titleBar {
  background: #fff;
  text-align: center;
  padding-top: 60rpx;
  height: 70rpx;
  font-size: 38rpx;
  font-family: fantasy;
  font-weight: 600;
}

.iconBar {
  position: absolute;
  left: 32rpx;
  width: 40rpx;
  height: 40rpx;
  top: 70rpx;
}

page, .page, .page__bd {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background: #f5ccd4;
}

.weui-navbar {
  background: #fff;
}

.panel1 {
  box-sizing: border-box;
  height: 110%;
  padding-top: 120rpx;
  margin-top: -10rpx;
  background: url('http://bmob-cdn-18960.b0.upaiyun.com/2018/07/10/9f9dab6c400d467f808d95abff90b07a.png');
  background-size: 100% 100%;
}

textarea {
  box-sizing: border-box;
  background: white;
  width: 542rpx;
  height: 542rpx;
  margin: 0 auto;
  border-radius: 40rpx;
  padding: 5rpx 0rpx 30rpx 0rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.2);
}

textarea.textarea1 {
  z-index: 2;
}

textarea.textarea2 {
  margin-top: -520rpx;
  width: 500rpx;
  z-index: 1;
}

textarea.textarea3 {
  margin-top: -520rpx;
  width: 460rpx;
  z-index: 0;
 
}

.placeholder {
  width: 30%;
  height: 80rpx;
  z-index: 3;
  position: absolute;
  left: 37%;
  top: 440rpx;
}

.rulian {
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  position: absolute;
  bottom: 270rpx;
  left: 270rpx;
}

.agree {
  font-size: 22rpx;
  text-align: center;
  position: absolute;
  bottom: 170rpx;
  left: 130rpx;
}
.agreeCheckbox{
  position: relative;
  top: -3px;
}
.agree checkbox {
  transform: scale(0.6) translateY(-4rpx);
  border: #aaacb0;
}

/* 查询誓言 */

.weui-tab__content {
  box-sizing: border-box;
  background: url('http://bmob-cdn-18960.b0.upaiyun.com/2018/07/10/9f9dab6c400d467f808d95abff90b07a.png');
  background-size: 100% 100%;
  height: 100%;
  width: 100%;
  padding-top: 100rpx;
  margin-top: -10rpx;
}

.sroll-table {
  background: #fff;
  width: 630rpx;
  height: 800rpx;
  margin: 0 auto;
  border-radius: 40rpx;
  padding-bottom: 130rpx;
}

/* 表格 */

.table {
  padding-top: 40rpx;

  
  font-weight: bold;
 
}

.inner-scroll {
  box-sizing: border-box;
  height: 800rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
}

.thead {
  display: flex;
  justify-content: space-around;
  font-size: 26rpx;
  height: 50rpx;
  border-bottom: 2rpx solid #adadad;
  margin-right: 40rpx;
  margin-left: 40rpx;
}

.thead .th {
  width: 27%;
 font-family: "方正舒体";
}

.thead .th:first-child {
  width: 35%;
}

.tbody {
  display: flex;
  justify-content: space-around;
  font-size: 24rpx;
  height: 72rpx;
  border-bottom: 2rpx solid #adadad;
}

.tbody .tr {
  color: #859ca3;
  display: flex;
  align-items: center;
  font-size: 22rpx;
  width: 27%;
  font-weight: 200;
  font-style:italic;
  /* font-family: "方正舒体"; */

}

.tbody .tr:first-child {
  color: #010101;
  font-size: 26rpx;
  font-weight: bold;
  width: 35%;
}

